{
  "name": "unmint",
  "version": "1.0.0",
  "private": true,
  "description": "A free, open-source Mintlify-style documentation system",
  "repository": {
    "type": "git",
    "url": "https://github.com/gregce/unmint.git"
  },
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev": "npm run dev --workspace=packages/template",
    "build": "npm run build --workspace=packages/template",
    "build:cli": "npm run build --workspace=packages/create-unmint",
    "test": "npm run test --workspace=packages/template"
  },
  "keywords": [
    "documentation",
    "docs",
    "mintlify",
    "nextjs",
    "mdx",
    "fumadocs"
  ],
  "author": "Greg Ceccarelli",
  "license": "MIT"
}
